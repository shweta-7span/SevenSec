<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/primary200" />
            <corners android:radius="@dimen/switch_thumb_radius" />
            <size android:width="@dimen/switch_thumb_width" android:height="@dimen/switch_thumb_height" />
            <stroke android:width="@dimen/switch_thumb_stroke_width" android:color="@color/switch_thumb_stoke_color" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/grey700" />
            <corners android:radius="@dimen/switch_thumb_radius" />
            <size android:width="@dimen/switch_thumb_width" android:height="@dimen/switch_thumb_height" />
            <stroke android:width="@dimen/switch_thumb_stroke_width" android:color="@color/switch_thumb_stoke_color" />
        </shape>
    </item>
</selector>